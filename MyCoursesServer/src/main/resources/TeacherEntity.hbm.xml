<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.j2ee.mycourses.entity.TeacherEntity" table="teacher_entity" schema="j2ee_mycourses">
        <id name="teacherNo">
            <column name="teacher_no" sql-type="varchar(230)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(255)"/>
        </property>
        <property name="teacherNo">
            <column name="teacher_no" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>
